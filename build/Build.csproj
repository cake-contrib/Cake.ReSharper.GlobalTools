<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RunWorkingDirectory>$(MSBuildProjectDirectory)</RunWorkingDirectory>
    <NoWarn>$(NoWarn);CC0091</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="ExternalAnnotations\**" />
    <Compile Remove="tools\**" />
    <EmbeddedResource Remove="ExternalAnnotations\**" />
    <EmbeddedResource Remove="tools\**" />
    <None Remove="ExternalAnnotations\**" />
    <None Remove="tools\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Cake.Frosting" Version="2.2.0" />
    <PackageReference Include="Cake.Issues" Version="1.0.0" />
    <PackageReference Include="Cake.Issues.DupFinder" Version="1.0.0" />
    <PackageReference Include="Cake.Issues.InspectCode" Version="1.0.0" />
    <PackageReference Include="Cake.Issues.MsBuild" Version="1.0.0" />
    <PackageReference Include="Cake.Issues.Reporting" Version="1.0.0" />
    <PackageReference Include="Cake.Issues.Reporting.Generic" Version="1.0.0" />
    <PackageReference Include="Cake.ReSharperReports" Version="0.11.1" />
    <PackageReference Include="Cake.Sonar" Version="1.1.30" />
    <PackageReference Include="JetBrains.Annotations" Version="2022.1.0" />
  </ItemGroup>
</Project>